{
  "name": "catalyst-education",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:staging": "NODE_ENV=staging vite build",
    "build:production": "NODE_ENV=production vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "format.fix": "prettier --write .",
    "typecheck": "tsc",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "deploy:staging": "bash scripts/deploy.sh staging",
    "deploy:production": "bash scripts/deploy.sh production",
    "docker:build": "docker build -t catalyst-education .",
    "docker:run": "docker run -d -p 80:80 --name catalyst-app catalyst-education",
    "health-check": "curl -f http://localhost:5173/health.json || exit 1",
    "setup:database": "node scripts/setup-database.mjs",
    "setup:auth": "node scripts/configure-auth.mjs",
    "setup:templates": "node scripts/configure-auth.mjs --templates",
    "setup:all": "npm run setup:database && npm run setup:auth",
    "postinstall": "echo '\nðŸŽ“ Catalyst Setup Complete!\n  Run: npm run setup:all\n  Then: npm run dev\n'"
